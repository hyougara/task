<% provide(:title ,"Task一覧") %>
<h1>Task一覧</h1>
<div class="micropost">
    <% @micropost.each do |micropost| %>
    <ul>
        <li><%= micropost.title %></li>
        <li><%= micropost.content %></li>
        <%= link_to "編集" ,edit_micropost_path(@micropost) %>
        <%= link_to "削除" ,edit_micropost_path(@micropost) %>
    </ul>
    <% end %>
</div>